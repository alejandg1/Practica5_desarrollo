{
  "info": {
    "name": "Practica4 API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Create User",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john.doe@example.com\",\n  \"password\": \"password123\"\n}"
        },
        "url": {
          "raw": "http://localhost:3000/users",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["users"]
        }
      }
    },
    {
      "name": "Get Users",
      "request": {
        "method": "GET",
        "url": {
          "raw": "http://localhost:3000/users",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["users"]
        }
      }
    },
    {
      "name": "Update User",
      "request": {
        "method": "PUT",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"Jane Doe\"\n}"
        },
        "url": {
          "raw": "http://localhost:3000/users/:id",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["users", ":id"]
        }
      }
    },
    {
      "name": "Delete User",
      "request": {
        "method": "DELETE",
        "url": {
          "raw": "http://localhost:3000/users/:id",
          "protocol": "http",
          "host": ["localhost"],
          "port": "3000",
          "path": ["users", ":id"]
        }
      }
    }
  ]
}